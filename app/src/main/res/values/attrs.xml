<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTextView">
        <attr name="customFont" format="string"/>
    </declare-styleable>
    <declare-styleable name="ProgressWheel">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="dimension" />
        <attr name="delayMillis" format="integer" />
        <attr name="circleColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="barWidth" format="dimension" />
        <attr name="pbarLength" format="dimension" />
    </declare-styleable>
    <declare-styleable name="util">
        <attr name="font_face">
            <enum name="regular" value="0" />
            <enum name="light" value="1" />
            <enum name="medium" value="2" />
            <enum name="black" value="3" />
            <enum name="bold" value="4" />
        </attr>
        <attr name="font_weight">
            <enum name="normal" value="0" />
            <enum name="light" value="1" />
            <enum name="bold" value="2" />
        </attr>
        <attr name="pattern" format="string" />
    </declare-styleable>
    <declare-styleable name="RCircularImageView">
        Â 
        <attr name="civ_border" format="boolean" />
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_background_color" format="color" />
        <attr name="civ_shadow" format="boolean" />
        <attr name="civ_shadow_color" format="color" />
        <attr name="civ_shadow_radius" format="float" />
        <attr name="civ_shadow_gravity">
